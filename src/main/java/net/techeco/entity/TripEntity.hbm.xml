<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.techeco.entity.TripEntity" table="Trip" schema="dbo" catalog="T1P">
        <id name="id" column="Id"/>
        <property name="name" column="Name"/>
        <property name="description" column="Description"/>
        <property name="startTime" column="StartTime"/>
        <property name="endTime" column="EndTime"/>
        <property name="gatheringPlace" column="GatheringPlace"/>
        <property name="isFinished" column="IsFinished"/>
        <property name="adminId" column="AdminId"/>
    </class>
</hibernate-mapping>